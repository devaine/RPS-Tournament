{
  "globalDependencies": ["tsconfig.json"],
  "ui": "stream",
  "tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["apps/frontend", "apps/api"],
			"outputs": ["apps/**/dist/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"host": {
			"dependsOn": ["^build"],
			"inputs": ["apps/api/dist"]
		}
  },
  "remoteCache": {}
}
